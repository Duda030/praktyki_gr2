<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainMenuActivity">

    <Button
        android:id="@+id/btnMojeObserwacje"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/moje_obserwacje"
        app:layout_constraintBottom_toTopOf="@+id/btnDodajGniazdo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnDodajGniazdo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/dodaj_gniazdo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnDodajObserwacje"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/dodaj_obserwacj"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnDodajGniazdo" />

</androidx.constraintlayout.widget.ConstraintLayout>